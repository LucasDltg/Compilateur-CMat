int main()
{
    int a = 1;
    int b = 2;
    int c = 3;

    print(a + b + c);
    print(a + b - c);
    print(a - b + c);

    // avec priorité
    print(a + b * c);
    print(a * b + c);
    print(a * b * c);

    // avec parenthèses
    print((a + b) * c);
    print(a * (b + c));
    print((a + b) * (a + c));

    
}