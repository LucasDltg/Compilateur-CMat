int main()
{
    matrix A[2][2];
    matrix B[2][2];

    A[0][0] = 1;
    A[0][1] = 2;
    A[1][0] = 3;
    A[1][1] = 4;

    B[0][0] = 1.5;
    B[0][1] = 2.5;
    B[1][0] = 3.5;
    B[1][1] = 4.5;

    printmat(3 * A - 3 * A + 5 * B - 5 * B + 1 * A);
    matrix C = (1 + 1) * A; // Error because of (1+1)
}