int main()
{
    // Testing multiplication
    matrix A[1][3];
    matrix B[3][1];
    matrix C[2][2];

    A[0][0] = 1;
    A[0][1] = 2;
    A[0][2] = 3;

    B[0][0] = 1.5;
    B[1][0] = 2.5;
    B[2][0] = 3.5;

    C[0][0] = 1;
    C[0][1] = 2;
    C[1][0] = 3;
    C[1][1] = 4;

    printmat(A * B);
    printmat(B * A);
    printmat(C * C);

    printf("\n");

    // Testing division
    printmat(A / B);
    printmat(B / A);
    printmat(C / C);

}