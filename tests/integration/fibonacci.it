Fibonacci sequence
=========
void main() {
    int fibo0 = 0, fibo1 = 1, fibo2 = 0;
    int number = 40, counter;

    counter = number;

    printf("0: ");
    print(fibo0);
    printf("\n");
    printf("1: ");
    print(fibo1);
    printf("\n");

    while(counter)
    {
        fibo2 = fibo0 + fibo1;

        print(number + 2 - counter);
        printf(": ");
        print(fibo2);
        printf("\n");

        fibo0 = fibo1;
        fibo1 = fibo2;

        counter = counter - 1;
    }
}
