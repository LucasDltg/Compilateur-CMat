Ternary
=========
void main() {
    int a = 1;
    int b = 2;
    int c = 3;

    matrix A[2][2] = {{1, 2}, {3, 4}};
    matrix B[2][2] = {{4, 3}, {2, 1}};

    print(a == 1 ? b : c);
    print(a == 0 ? b : c);
    printf("\n");
    printmat(a == 1 ? A : B);
    printmat(a == 0 ? A : B);

}
