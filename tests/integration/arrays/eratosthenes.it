Sieve of Eratosthenes
=========
void main() {
    int data[100];
    int len = 100;
    int i, j;

    // Initialize the array to 1
    for(i = 0; i < len; i++)
        data[i] = 1;

    // Calculate
    for(i = 2; i <= 10; i++) // SQRT of len (can't calculate it lol)
        if (data[i])
            for(j = i * i; j < len; j = j + i)
                data[j] = 0;

    // Print
    for(i = 0; i < len; i++) {
        if (data[i]) {
            print(i);
            printf("\n");
        }
    }
}
